              "CC-BY-NC-SA-2.5",
              "CC-BY-NC-SA-3.0",
              "CC-BY-NC-SA-3.0-US",
              "CC-BY-NC-SA-4.0",
              "CC-BY-SA-1.0",
              "CC-BY-SA-2.0",
              "CC-BY-SA-2.5",
              "CC-BY-SA-3.0",
              "CC-BY-SA-3.0-AT",
              "CC-BY-SA-3.0-US",
              "CC-BY-SA-4.0",
              "CC0-1.0",
              "CDDL-1.0",
              "CECILL-2.1",
              "CNRI-Python",
              "CPAL-1.0",
              "CPL-1.0",
