          "Training & Employment",
          "Transition",
          "Transparency & Accountability",
          "Travel and Hospitality",
          "Violent Extremism",
          "Water and Sanitation",
          "Workforce",
          "World Population",
          "Youth"
        ]
      }
    },
    "type": {
      "type": "array",
      "description": "What category best describes this project?",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "type": "string",
        "enum": [
          "software",
          "data",
          "content",
          "standard",
          "aimodel"
        ]
      }
    },
    "repositories": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "url"
        ],
        "properties": {
          "name": {
            "type": "string",
            "description": "Name of the Github (or other) repository"
          },
          "url": {
            "type": "string",
            "description": "Website of the repository"
          }
        }
      }
    },
    "organizations": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Name of the organization"
          },
          "website": {
            "type": "string",
            "description": "Website of the organization"
          },
          "org_type": {
            "type": "string",
            "description": "Type of organization",
            "enum": [
              "owner",
              "maintainer",
              "funder",
              "implementer"
            ]
          },
          "contact_name": {
            "type": "string",
            "description": "Name of contact individual in the organization"
          },
          "contact_email": {
            "type": "string",
            "description": "Email for contact individual in the organization"
          }
        }
      }
    },
    "stage": {
      "type": "string",
      "description": "Review stage of Digital Public Good",
      "enum": [
        "nominee",
        "DPG",
        "archive"
      ]
    }
