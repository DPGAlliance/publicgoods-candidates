{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "",
  "$comment": "Digital Public Good Metaschema: JSON Schema",
  "title": "Digital Public Good",
  "type": "object",
  "required": [
    "name",
    "description",
    "license",
    "organizations",
    "SDGs",
    "type",
    "stage"
  ],
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the Digital Public Good. Full name, no initialisms, no acronyms."
    },
    "aliases": {
      "type": "array",
      "description": "Abbreviations, initialisms, or alternate names for Digital Public Good, where relevant.",
      "uniqueItems": true,
      "items": {
        "type": "string"
      }
    },
    "description": {
      "type": "string",
      "description": "Concise 1-line description of Global Digital Public Good"
    },
    "website": {
      "type": "string",
      "description": "Public website for the Global Digital Public Good."
    },
    "license": {
      "type": "array",
      "description": "License(s) under which the Global Digital Pubic Good is released.",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "type": "object",
        "required": [
          "spdx",
          "licenseURL"
        ],
        "properties": {
